"xbacklight -dec 5"
	XF86MonBrightnessDown

"xbacklight -inc 5"
	XF86MonBrightnessUp

"pamixer --increase 5" 
	XF86AudioRaiseVolume

"pamixer --decrease 5" 
	XF86AudioLowerVolume

"pamixer --toggle-mute"
	XF86AudioMute

"/usr/bin/i3lock -t -i /data/wallpapers/orion-nebula-2560x1440.png"
	Control + End
